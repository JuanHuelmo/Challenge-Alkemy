@model IEnumerable<Challenge.Models.Materia>

@{
    ViewBag.Title = "ListadoMateriasInscripto";
}

<h3>Listado Materias Inscripto</h3>


<table class="table">
    <tr>
   
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeFrom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeTo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quota)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Inscriptos)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeFrom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeTo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quota)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Inscriptos)
        </td>
        <td>

            @Html.ActionLink("Eliminar Inscripcion", "EliminarInscripcion", new { id=item.Id }) |
        </td>
    </tr>
}

</table>
